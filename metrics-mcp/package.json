{
  "name": "metricshub-mcp-server",
  "version": "1.0.0",
  "description": "MetricsHub - Analytics Dashboard MCP server with API key authentication and role-based access control",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/streamable-http.js",
    "dev": "tsc && node dist/index.js",
    "dev:http": "tsc && node dist/streamable-http.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "analytics",
    "dashboard",
    "metrics",
    "model-context-protocol",
    "authentication",
    "rbac",
    "sse",
    "streamable-http"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
